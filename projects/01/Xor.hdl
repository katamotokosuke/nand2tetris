/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(a=a, out=nota);
    Not(b=b, out=notb);
    And(a=a, b=notb, out=v1);
    And(a=nota, b=b, out=v2);
    Or(a=v1, b=v2, out=out);
}